            <string>///Jump and collisons

//sets keypress
var jkeypressed = keyboard_check_pressed(vk_space);
var jkeyheld = keyboard_check(vk_space);    


//collison with top of objects
if(inPit==0)
if(place_meeting(x, y+1, objGround) or place_meeting(x, y+1, objColumn1Top)
or place_meeting(x, y+1, objPlatformTop))
{
vspd=0;

//jumping
    if(jkeypressed &amp;&amp; sprite_index != sprRaptorDie &amp;&amp; sprite_index != sprRaptorDead){       
        vspd = -jspd;
        audio_play_sound(sndJump,1,false)
    }
}
else{
//gravity
if(vspd &lt; 10){
    vspd+=grav;
}
}
//variable jump height
if(vspd &lt; 0) &amp;&amp; (!jkeyheld){
    vspd = max(vspd, 0)
}

//vspeed = vspd;

//collision with columns
if( sprite_index == sprRaptorRun )
{
if (place_meeting(x+hspeed,y-8,objColumnParent) or place_meeting(x+hspeed, y-8, objPlatform))
{
    hspeed = -5;
    sprite_index = sprRaptorDie;
    image_index = 0;
    image_speed = 0.5;     
}
}
//collision with Little Man    
if distance_to_object(objLittleManRun) &lt; 2
{
    room_goto(Level1_Death)
}
//raptor die
if( sprite_index == sprRaptorDie)
{  
      if(image_index &gt;=18 ){
        sprite_index = sprRaptorDead;
        audio_play_sound(sndYouDied,1,false)
        alarm[0] = 90;
      }
}

//collison with spike pit
if (place_meeting(x+hspeed,y,objSpikePitTop) &amp;&amp; inPit = 0) 
{
    inPit=1;
    vspd= 7;
    hspeed = -5;
    show_debug_message(vspeed)
    show_debug_message(vspd)    
}

//collison with spike pit top
if (place_meeting(x+hspeed,y,objSpikePitTop2)) 
{
    vspd = 0;
    hspeed = -5;
    sprite_index = sprRaptorDie;
    image_index = 0;
    image_speed = 0.5;   
    with(objSpikePitTop2){
        instance_destroy();
    }  
}

vspeed = vspd;
     
     
            

</string>
